{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "eslint:recommended", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "radix": "off",
    "no-plusplus": "off",
    "no-unused-vars": "warn",
    "no-use-before-define": "warn"
  },
  "ignorePatterns": ["**/node_modules/**"]
}
